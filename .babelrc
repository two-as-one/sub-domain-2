{
  "presets": [["@babel/preset-env"]],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-syntax-object-rest-spread"
  ],
  "sourceMaps": true,

  "env": {
    "production": {
      "sourceMaps": false
    }
  }
}
